            android:label="Sanus">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Perfil"></activity>
        <activity android:name=".Register"></activity>
        <activity android:name=".Menu"></activity>
        <activity android:name=".CargarRecetas"></activity>
        <activity android:name=".VerReceta"></activity>
        <activity android:name=".Editar"></activity>
        <activity android:name=".Inicio"></activity>
        <activity android:name=".Ayuda"></activity>
    </application>

</manifest>
